{
    "logging": {
        "level": "ALL"
    },
    "ledInfo": {
        "brightness": 75,
        "cloud": 25,
        "wifi": 26,
        "power": 27
    },
    "gpsSensor": {
        "enabled": true,
        "tx": 22,
        "rx": 23,
        "baud": 9600,
        "sampleRate": 5000
    },
    "envSensor": {
        "enabled": true,
        "data": 14,
        "scale": 1,
        "sampleRate": 20000
    },
    "device": {
        "prefix": "OT",
        "wakeup": 1200,
        "sleep": 60,
        "location": "<UNKNOWN>"
    },
    "cloud": {
        "connect": "azure",
        "certs": {
            "certificate": "/cloud/cert.pem",
            "key": "/cloud/key.pem"
        },
        "iotHub": {
            "endpoint": "",
            "name": "",
            "port": 8883,
            "sendTelemetry": true,
            "intervalSeconds": 45
        },
        "azure": {
            "ca": "/cloud/portal-azure-com.pem"
        },
        "aws": {
            "ca": "/cloud/console-aws-com.pem"
        }
    }
}